{
    "presets": [
        "@babel/preset-react",
        [
            "@babel/env",
            {
                "useBuiltIns": "usage",
                "shippedProposals": true,
                "spec": true,
                "debug": false,
                "corejs": 3
            }
        ]
    ],
    "plugins": [
        "react-hot-loader/babel",
        ["@babel/transform-runtime", {
            "regenerator": true
        }],
        ["babel-plugin-styled-components", {
            "displayName": true,
            "fileName": false
        }]
    ]
}
